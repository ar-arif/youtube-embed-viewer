{
  "name": "Youtube Embed Viewer",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "",
    "default_icon": {
      "50": "icons/icons8-tv-50.png",
      "100": "icons/icons8-tv-100.png"
    }
  },
  "icons": {
    "50": "icons/icons8-tv-50.png",
    "100": "icons/icons8-tv-100.png"
  }
}
